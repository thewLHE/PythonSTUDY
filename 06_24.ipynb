    "#solv\n",
    "seqA=\"ATGTCGACTA\"\n",
    "seqB=\"AAATCGCGCGTGCTG\"\n",
    "\n",
    "m,n=len(seqA),len(seqB)\n",
    "\n",
    "s0=[0 for i in range(n)]\n",
    "Sij=[]\n",
    "for i in range(m):\n",
    "    Sij.append(s0.copy())\n",
    "#Initialize\n",
    "for i in range(n):\n",
    "    Sij[0][i]=-3*i\n",
    "for j in range(m):\n",
    "    Sij[j][0]=-3*j\n",
    "\n",
    "def w(a,b):\n",
    "    if(a==b and a!=\"-\"):\n",
    "        return 8\n",
    "    elif(a==\"-\" or b==\"-\"):\n",
    "        return -3\n",
    "    else:\n",
    "        return -5\n",
    "\n",
    "for i in range(m-1):\n",
    "    for j in range(n-1):\n",
    "        print(seqA[i],seqB[j],w(seqA[i],seqB[j]),w(seqA[i],'-'),w('-',seqB[j]))\n",
    "        #Sij[i+1][j+1]=max(w(seqA[i+1],seqB[j+1]),w(seqA[i+1],'-'),w('-',seqB[j+1]))\n",
    "        Sij[i+1][j+1]=max( Sij[i][j]+w(seqA[i],seqB[j]),\\\n",
    "                          Sij[i][j-1]+w(seqA[i],\"-\"),\\\n",
    "                          Sij[i-1][j]+w('-',seqB[j]) )\n",
    "        \n",
    "print(Sij)"
   ]
