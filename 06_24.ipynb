    "#an optimal alignment -7p\n",
    "\"\"\"A=[\"C\",\"G\",\"G\",\"A\",\"T\",\"C\",\"A\",\"T\"]\n",
    "B=[\"C\",\"T\",'T','A',\"A\",\"C\",\"T\"]\n",
    "#[[len(A)],[len(B)]]\n",
    "S=[\n",
