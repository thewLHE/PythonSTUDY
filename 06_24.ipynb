    "    ,[-6]\n",
    "    ,[-9]\n",
    "    ,[-12]\n",
    "    ,[-15]\n",
    "    ,[-18]\n",
    "    ,[-21]\n",
    "]\n",
    "#print(S[0][1])\n",
    "def w(a,b):\n",
    "    if(a==b):\n",
    "        return 8\n",
    "    elif(a!=b ):\n",
    "        return -5\n",
    "    else:\n",
    "        print(\"error\")\n",
    "def s(a,b):#insert value-cal\n",
    "    if(a==\"-\" ):\n",
    "        a=0\n",
    "    elif( b==\"-\"):\n",
    "        b=0\n",
    "    c=a-b\n",
    "    return c\n",
    "#print(range(len(S[0])-1)) #range(0, 8)\n",
    "q,v=0,0\n",
    "for i in range(len(S[0])):#x의 값\n",
    "    \n",
    "        for j in range(len(S[i])):#y의 값\n",
    "            s1=s(S[i][j],S[i][j+1])\n",
    "            s2=s(S[i][j],S[i+1][j])\n",
    "            wv=w(A[q],B[v])\n",
    "            res=max(s1,s2,wv)\n",
    "            S[i][j].append(res)\n",
    "            q+=1;v+=1\"\"\""
